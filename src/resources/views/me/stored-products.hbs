<div class="mt-4">
    <h3>My products</h3>
    <table class="table">
        <thead>
            <tr>
                <th scope="col">#</th>
                <th scope="col">Name Product</th>
                <th scope="col">Video ID</th>
                <th scope="col" colspan="2">Created At</th>
            </tr>
        </thead>
        
        <tbody>
            {{#each phones}}
            <tr>
                <th scope="row">{{sum  @index 1}}</th>
                <td>{{this.title}}</td>
                <td>{{this.videoId}}</td>
                <td>{{this.createdAt}}</td>
                <td>
                    <a href="/phones/{{this._id}}/edit" class="btn btn-link">Sửa</a>
                    <a href="" class="btn btn-link">Xóa</a>
                </td>
            </tr>
            {{/each}}
        </tbody>
    </table>
</div>